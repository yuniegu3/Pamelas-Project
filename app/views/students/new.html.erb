<%= form_for @student do |f| %>  
    <%= f.label :firstname %>  
    <%= f.text_field :firstname %>  
    <%= f.label :lastname %>  
    <%= f.text_field :lastname %>
    <%= f.label :age %>  
    <%= f.number_field :age %> 
    <%= f.label :education %>  
    <%= f.text_field :education %>  
    <%= f.label :cohort_id %>  
    <%= f.number_field :cohort_id %>
    <%# maybe make a drop down menu with cohort name instead of id %>
    <%= f.submit %>
<% end %>

<% @error %>