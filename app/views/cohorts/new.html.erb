<%= form_for @cohort do |f| %>  
    <%= f.label :name %>  
    <%= f.text_field :name %>  
    <%= f.label :start_date %>  
    <%= f.text_field :start_date %>
    <%= f.label :end_date %>  
    <%= f.text_field :end_date %> 
    <%= f.label :instructor_id %>  
    <%= f.number_field :instructor_id %> 
    <%= f.label :course_id %>  
    <%= f.number_field :course_id %> 
    <%= f.submit %>
<% end %>

<% @error %>